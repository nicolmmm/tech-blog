
<h2 class="dashboard">Dashboard</h2>


{{#each userDataPlain.blogs as |blog| }}
<div class="postView row align-center mb-2">
  <div class="col-md-8">
  <a href="/blog/{{blog.id}}">
  <h3>{{{blog.title}}}</h3></a>
  <p> {{blog.body}}</p>
  <p class="createdAt">{{format_date blog.createdAt}}</p>
  
  </div>
</div>
{{/each}}
